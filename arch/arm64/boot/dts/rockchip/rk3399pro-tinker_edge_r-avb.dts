/dts-v1/;
#include "rk3399pro-tinker_edge_r.dtsi"

/ {
	model = "Tinker Edge R";
	compatible = "rockchip,rk3399pro-evb-v11-avb", "rockchip,rk3399pro";

	chosen: chosen {
		bootargs = "console=ttyFIQ0 androidboot.baseband=N/A androidboot.wificountrycode=US androidboot.veritymode=enforcing androidboot.hardware=rk30board androidboot.console=ttyFIQ0 firmware_class.path=/vendor/etc/firmware init=/init rootwait ro init=/init androidboot.selinux=permissive";
	};
};

&firmware_android {
	compatible = "android,firmware";
	boot_devices = "fe330000.sdhci";
	fstab {
		compatible = "android,fstab";
		vendor {
			compatible = "android,vendor";
			dev = "/dev/block/by-name/vendor";
			type = "ext4";
			mnt_flags = "ro,barrier=1,inode_readahead_blks=8";
			fsmgr_flags = "wait";
		};
	};
};

&i2c1 {
	status = "okay";

	cam_sensor1: cam_sensor1@36 {
		compatible = "asus,cam_sensor";
		status = "okay";
		reg = <0x36>;
		clocks = <&cru SCLK_TESTCLKOUT2>;
		clock-names = "xvclk";

		/* conflict with csi-ctl-gpios */
		power-gpios = <&gpio0 RK_PB0 GPIO_ACTIVE_HIGH>;
		enable-gpios = <&gpio2 RK_PA3 GPIO_ACTIVE_HIGH>;
		clksel-gpios = <&gpio4 RK_PD6 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "camera_default";
		pinctrl-0 = <&test_clkout2>;
		i2c-index = <0>;

		port {
			ov5647_out: endpoint {
				remote-endpoint = <&mipi_in_ucam0>;
				data-lanes = <1 2>;
			};
		};
	};
};

&i2c2 {
	status = "okay";

	cam_sensor2: cam_sensor2@36 {
		compatible = "asus,cam_sensor";
		status = "okay";
		reg = <0x36>;
		clocks = <&cru SCLK_CIF_OUT>;
		clock-names = "xvclk";

		/* conflict with csi-ctl-gpios */
		power-gpios = <&gpio2 RK_PB3 GPIO_ACTIVE_HIGH>;
		enable-gpios = <&gpio2 RK_PA2 GPIO_ACTIVE_HIGH>;
		i2c-index = <1>;

		port {
			imx219_out: endpoint {
				remote-endpoint = <&mipi_in_ucam1>;
				data-lanes = <1 2>;
			};
		};
	};
};

&isp0_mmu {
	status = "okay";
};

&isp1_mmu {
	status = "okay";
};

&isp0 {
	status = "disabled";
};

&mipi_dphy_rx0 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipi_in_ucam0: endpoint@1 {
				reg = <1>;
				remote-endpoint = <&ov5647_out>;
				data-lanes = <1 2>;
			};
		};

		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			dphy_rx0_out: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&isp0_mipi_in>;
			};
		};
	};
};

&rkisp1_0 {
	status = "okay";

	port {
		#address-cells = <1>;
		#size-cells = <0>;

		isp0_mipi_in: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&dphy_rx0_out>;
		};
	};
};

&pinctrl {
	isp {
		test_clkout2: cif-test_clkout2 {
			rockchip,pins =
				/* test_clkout2 */
				<0 8 RK_FUNC_3 &pcfg_pull_none>;
		};
	};
};

&i2c4 {
	status = "okay";
	i2c-scl-rising-time-ns = <345>;
	i2c-scl-falling-time-ns = <11>;

	vm149c: vm149c@0c {
		compatible = "silicon touch,vm149c";
		status = "disabled";
		reg = <0x0c>;
		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "back";
	};

	ov13850: ov13850@10 {
		compatible = "ovti,ov13850";
		status = "disabled";
		reg = <0x10>;
		clocks = <&cru SCLK_CIF_OUT>;
		clock-names = "xvclk";

		/* conflict with csi-ctl-gpios */
		reset-gpios = <&gpio4 RK_PD2 GPIO_ACTIVE_HIGH>;
		pwdn-gpios = <&gpio4 RK_PD1 GPIO_ACTIVE_HIGH>;
		pinctrl-names = "rockchip,camera_default";
		pinctrl-0 = <&cif_clkout>;
		rockchip,camera-module-index = <0>;
		rockchip,camera-module-facing = "back";
		rockchip,camera-module-name = "CMK-CT0116";
		rockchip,camera-module-lens-name = "Largan-50013A1";
		lens-focus = <&vm149c>;

		port {
			ucam_out1: endpoint {
				remote-endpoint = <0>;
				data-lanes = <1 2>;
			};
		};
	};
};

&isp1 {
	status = "disabled";
};

&mipi_dphy_tx1rx1 {
	status = "okay";

	ports {
		#address-cells = <1>;
		#size-cells = <0>;

		port@0 {
			reg = <0>;
			#address-cells = <1>;
			#size-cells = <0>;

			mipi_in_ucam1: endpoint@1 {
				reg = <1>;
				remote-endpoint = <&imx219_out>;
				data-lanes = <1 2>;
			};
		};

		port@1 {
			reg = <1>;
			#address-cells = <1>;
			#size-cells = <0>;

			dphy_tx1rx1_out: endpoint@0 {
				reg = <0>;
				remote-endpoint = <&isp1_mipi_in>;
			};
		};
	};
};

&rkisp1_1 {
	status = "okay";

	port {
		#address-cells = <1>;
		#size-cells = <0>;

		isp1_mipi_in: endpoint@0 {
			reg = <0>;
			remote-endpoint = <&dphy_tx1rx1_out>;
		};
	};
};
